class Червоний:
  def __str__(self):
      return "Це червоний колір."

class Синій:
  def __str__(self):
      return "Це синій колір."

class Зелений:
  def __str__(self):
      return "Це зелений колір."
    
class Помаранчевий(Червоний):
  def __str__(self):
      return "Це помаранчевий колір."

class Фіолетовий(Синій):
  def __str__(self):
      return "Це фіолетовий колір."
червоний = Червоний()
синій = Синій()
зелений = Зелений()
помаранчевий = Помаранчевий()
фіолетовий = Фіолетовий()

print(червоний)
print(синій)
print(зелений)
print(помаранчевий)
print(фіолетовий)
